    <div class="col-sm-3 col-md-2 sidebar">
        <h1 class="text-center">Leerlingen</h1>
        <hr />
        <div>
            <ul class="nav nav-sidebar">
                <li class="@(ViewBag.id == null ? "active" : null)"><a href="~/Home/Index"><i class="fa fa-child" aria-hidden="false"></i> all students</a></li>
                @foreach (var x in ViewBag.classClass.Students)
                {
                    <li class="@(ViewBag.id == x.Id ? "active" : null)"><a href="~/Home/Index/@x.Id"><i class="fa fa-child" aria-hidden="false"></i> @x.Name</a></li>
                }
            </ul>
        </div>
    </div>
