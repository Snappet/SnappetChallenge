{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Console",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/RReporter.Run.Console/bin/Debug/netcoreapp2.1/RReporter.Run.Console.dll",
            "args": [],
            "cwd": "${workspaceFolder}",
            "stopAtEntry": false,
            "console": "internalConsole"
        },
        {
            "type": "chrome",
            "request": "launch",
            "name": "WebApp",
            "url": "http://localhost:4200",
            "webRoot": "${workspaceFolder}/RReporterWebApp"
        },
        {
            "name": "Web Api",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/RReporter.Run.WebApi/bin/Debug/netcoreapp2.1/RReporter.Run.WebApi.dll",
            "args": [],
            "cwd": "${workspaceFolder}",
            "stopAtEntry": false,
            "launchBrowser": {
                "enabled": true,
                "args": "${auto-detect-url}/api"
            },
            "env": {
                "ASPNETCORE_ENVIRONMENT": "Development"
            },
        }
    ]
}